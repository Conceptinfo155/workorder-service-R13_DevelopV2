{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "workOrderNumber",
    "newWorkOrderNumber",
    "errors",
    "status"
  ],
  "properties": {
    "workOrderNumber": {
      "$id": "#/properties/workOrderNumber",
      "type": "string",
      "title": "The Workordernumber Schema",
      "default": "",
      "examples": [
        "crsr0989"
      ]
    },
    "newWorkOrderNumber": {
      "$id": "#/properties/newWorkOrderNumber",
      "type": "string",
      "title": "The Newworkordernumber Schema",
      "default": "",
      "examples": [
        "9890rsrc"
      ]
    },
    "errors": {
      "$id": "#/properties/errors",
      "type": "array",
      "title": "The Errors Schema",
      "items": {
        "$id": "#/properties/errors/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "code",
          "message"
        ],
        "properties": {
          "code": {
            "$id": "#/properties/errors/items/properties/code",
            "type": "string",
            "title": "The Code Schema",
            "default": "",
            "examples": [
              "UPDATE_WORKORDER_FAILED_NEW_WORKORDER_CREATED"
            ],
            "pattern": "^(.*)$"
          },
          "message": {
            "$id": "#/properties/errors/items/properties/message",
            "type": "string",
            "title": "The Message Schema",
            "default": "",
            "examples": [
              "107 - Failure to update workorder"
            ]
          }
        }
      }
    },
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "title": "The Status Schema",
      "default": "",
      "examples": [
        "SUCCESS"
      ]
    }
  }
}