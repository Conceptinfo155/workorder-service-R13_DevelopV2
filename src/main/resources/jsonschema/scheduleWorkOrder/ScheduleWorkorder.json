{
   "type":"object",
   "required":true,
   "properties":{
      "ScheduleWorkorderRequest":{
         "type":"object",
         "required":true,
         "properties":{
            "workOrderNumber":{
               "type":"string",
               "required":false,
               "minLength":1,
               "maxLength":20,
               "example":"SR0980970009898"
            },
            "ticketNumber":{
               "type":"string",
               "required":false,
               "maxLength":14,
               "example":"SR0980970009898"
            },
            "requestType":{
               "type":"string",
               "required":true,
               "enum":[
                  "NEW_REQUEST",
                  "RETRY_WORKORDER"
               ],
               "example":"NEW_REQUEST"
            },
            "noCrTicketIndicator":{
               "type":"boolean",
               "example":false
            },
            "noScheduleIndicator":{
               "type":"boolean",
               "example":false
            },
            "solutionDetails":{
               "type":"array",
               "items":{
                  "type":"object",
                  "required":["solutionType", "orderType"],
                  "properties":{
                     "solutionType":{
                        "type":"string",
                        "required":true,
                        "example":"ENS"
                     },
                     "orderType":{
                        "type":"string",
                        "required":true,
                        "example":"TC"
                     },
                     "transportType":{
                        "type":"string",
                        "required":false,
                        "example":"OTT"
                     }
                  }
               }
            },
            "bookAppointment":{
               "type":"object",
               "required":false,
               "properties":{
                  "optionId":{
                     "type":"string",
                     "required":false,
                     "maxLength":20,
                     "example":"09876"
                  },
                  "forceBookInd":{
                     "type":"boolean",
                     "example":true
                  },
                  "reservationId":{
                     "required":false,
                     "type":"string",
                     "maxLength":19,
                     "example":"09876"
                  }
               }
            },
            "createWorkOrder":{
               "type":"object",
               "required":true,
               "properties":{
                  "job":{
                     "type":"object",
                     "properties":{
                        "scheduleDate":{
                           "type":"string",
                           "required":false,
                           "maxLength":10,
                           "example":"2015-10-23"
                        },
                        "timeSlotCd":{
                           "type":"string",
                           "required":false,
                           "maxLength":3,
                           "example":"2"
                        },
                        "callFirstPhoneNum":{
                           "type":"string",
                           "minLength":1,
                           "maxLength":20,
                           "example":"4136500222"
                        },
                        "troubleCallIndicator":{
                           "type":"string",
                           "enum":[
                              "R",
                              "Q",
                              "B"
                           ],
                           "example":"R"
                        },
                        "jobComment":{
                           "type":"string",
                           "minLength":1,
                           "maxLength":4000,
                           "example":"Job Comments"
                        },
                        "orderComment":{
                           "type":"string",
                           "minLength":1,
                           "maxLength":500,
                           "example":"Order Comments"
                        },
                        "jobReasonCode":{
                           "type":"string",
                           "minLength":1,
                           "maxLength":500,
                           "example":"D0"
                        },
                        "technicianNum":{
                           "type":"string",
                           "minLength":1,
                           "maxLength":20,
                           "example":"0101"
                        }
                     }
                  },
                  "jobCustomer":{
                     "type":"object",
                     "properties":{
                        "customerId":{
                           "type":"string",
                           "required":true,
                           "minLength":1,
                           "maxLength":10,
                           "example":"4136500222"
                        },
                        "siteId":{
                           "type":"string",
                           "required":true,
                           "minLength":1,
                           "maxLength":20,
                           "example":"Site_0981098"
                        },
                        "lastName":{
                           "type":"string",
                           "required":true,
                           "minLength":1,
                           "maxLength":50,
                           "example":"Venkata"
                        },
                        "firstName":{
                           "type":"string",
                           "minLength":1,
                           "maxLength":50,
                           "example":"Harish"
                        },
                        "homePhoneNum":{
                           "type":"string",
                           "minLength":1,
                           "maxLength":20,
                           "example":"4136500222"
                        },
                        "workPhoneNum":{
                           "type":"string",
                           "minLength":1,
                           "maxLength":20,
                           "example":"4136500222"
                        },
                        "emailAddr":{
                           "type":"string",
                           "minLength":1,
                           "maxLength":320,
                           "example":"admin@abc.com"
                        }
                     }
                  },
                  "jobLocation":{
                     "type":"object",
                     "properties":{
                        "address":{
                           "type":"object",
                           "properties":{
                              "addrLine1":{
                                 "type":"string"
                              },
                              "addrLine2":{
                                 "type":"string"
                              },
                              "city":{
                                 "type":"string"
                              },
                              "state":{
                                 "type":"string"
                              },
                              "zipCode":{
                                 "type":"string"
                              }
                           }
                        },
                        "addrId":{
                           "type":"string"
                        }
                     }
                  }
               }
            }
         }
      }
   }
}