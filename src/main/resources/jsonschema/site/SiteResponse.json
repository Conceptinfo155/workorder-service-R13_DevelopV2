{
	"$schema": "http://json-schema.org/draft-06/schema#",
	"definitions": {
		
	},

		"type": "object",
		"properties": {
			"siteId": {
				"type": "string",
				"description": "The Amdocs Site ID",
				"example": "Site_1246_1",
				"position": 1
			},
			"siteName": {
				"position": 2,
				"description": "The name of the site.",
				"example": "sit1",
				"type": "string"
			},
			"siteType": {
				"position": 3,
				"description": "The site type - (ST,MT,MC,CT,CS,CH,DC,CST,CMT,TW,MSC,ML,OT) (Single Tenant, Multi-Tenant, Mobile Switching Center, Cell Tower, Cell Site, Carrier Hotel, Data Center, Commercial Single Tenant, Commercial Multi-tenant, Teleworker, MSC/HUB, Multi-Level, Other )",
				"example": "ST",
				"type": "string"
			},
			"customerGroupId": {
				"position": 4,
				"description": "Customer group Id",
				"example": "???",
				"type": "string"
			},
			"numberOfEmployees": {
				"position": 5,
				"description": "The number of employees at this site",
				"example": 11,
				"type": "integer"
			},
			"sitePhoneNumber": {
				"position": 6,
				"description": "The phone Number of the site",
				"example": "???",
				"type": "string"
			},
			"siteAddress": {
				"position": 7,
				"type": "object",
				"properties": {
					"addressLine1": {
						"description": "The street first line",
						"example": "300 E Main Street",
						"position": 1,
						"type": "string"
					},
					"addressLine2": {
						"description": "The street second line",
						"example": "Unit 1",
						"position": 2,
						"type": "string"
					},
					"city": {
						"description": "The city of the address",
						"example": "Philadelphia",
						"position": 3,
						"type": "string"
					},
					"state": {
						"description": "The state or province of the address",
						"example": "PA",
						"position": 4,
						"type": "string"
					},
					"country": {
						"description": "The country code of the address",
						"example": "US",
						"position": 5,
						"type": "string"
					},
					"zipCode": {
						"description": "The postal code of the address",
						"example": "19355",
						"position": 6,
						"type": "string"
					},
					"latitude": {
						"description": "The latitude in decimal degrees of the address",
						"example": "91.09",
						"position": 7,
						"type": "string"
					},
					"longitude": {
						"description": "The longitude in decimal degrees of the address",
						"example": "98.10",
						"position": 8,
						"type": "string"
					},
					"naxId": {
						"description": "NAX ID",
						"position": 9,
						"example": "120980149274",
						"type": "string"
					}
				}
			},
			"siteContacts": {
				"position": 8,
				"type": "array",
				"items": {
					"type": "object",
					"properties": {
						"contactId": {
							"description": "The Amdocs Contact ID",
							"example": "124378769",
							"position": 1,
							"type": "string"
						},
						"firstName": {
							"description": "The first (given) name of the individual",
							"example": "Sansa",
							"position": 2,
							"type": "string"
						},
						"lastName": {
							"description": "The family (last) name of the individual",
							"example": "Stark",
							"position": 3,
							"type": "string"
						},
						"middleName": {
							"description": "The middle name of the individual",
							"example": "Eddard",
							"position": 4,
							"type": "string"
						},
						"displayName": {
							"description": "The formatted name of the Individual",
							"example": "Sansa Stark",
							"position": 5,
							"type": "string"
						},
						"title": {
							"description": "Title for individuals name. Eg. Mr. Dr. etc",
							"example": "Mr",
							"position": 6,
							"type": "string"
						},
						"role": {
							"description": "The Role of the user. If Primary user - It would have value as Primary. ",
							"example": "Primary",
							"position": 7,
							"type": "string"
						},
						"preferredContactMethod": {
							"description": "The preferred contact method of the individual - Email, Phone, Fax, Postal",
							"example": "Email",
							"position": 8,
							"type": "string"
						},
						"preferredContactPeriod": {
							"description": "The preferred contact period of the individual - For Example: Morning, Afternoon, Evening, Midday",
							"example": "Morning",
							"position": 9,
							"type": "string"
						},
						"preferredContactTime": {
							"description": "The preferred contact time of the individual - Morning, Evening, Afternoon",
							"example": "Morning",
							"position": 10,
							"type": "string"
						},
						"preferredLanguage": {
							"description": "The preferred Language for the contact",
							"example": "English (United States)",
							"position": 11,
							"type": "string"
						},
						"contactMethods": {
							"position": 12,
							"type": "array",
							"items": {
								"type": "object",
								"properties": {
									"contactMethodType": {
										"description": "Contact medium type: Email, Fax, Mobile",
										"example": "Email",
										"type": "string"
									},
									"emailAddress": {
										"description": "Email id",
										"example": "emailid@email.com",
										"type": "string"
									},
									"extension": {
										"description": "Phone or fax extension number",
										"example": "1223",
										"type": "string"
									},
									"number": {
										"description": "Phone or fax number",
										"example": "9789982312",
										"type": "string"
									}
								}
							}
						}
					}
				}
			},
			"market": {
				"position": 9,
				"description": "The market of the site",
				"example": "???",
				"type": "string"
			},
			"submarket": {
				"position": 10,
				"description": "The site sub-market",
				"example": "???",
				"type": "string"
			},
			"region": {
				"position": 11,
				"description": "The region of the site",
				"example": "???",
				"type": "string"
			},
			"division": {
				"position": 12,
				"description": "The division of the site",
				"example": "???",
				"type": "string"
			},
			"areaType": {
				"position": 13,
				"description": "The area type",
				"example": "Please Specify",
				"type": "string"
			},
			"timeZoneInfo": {
				"type": "object",
				"properties": {
					"timeZone": {
						"type": "string",
						"description": "The TimeZone",
						"example": "Central Daylight Time"
					},
					"utc": {
						"type": "integer",
						"description": "The UTC Time Zone",
						"example": "-5"
					},
					"dst": {
						"type": "boolean",
						"description": "The Elocmsagsourcename Schema ",
						"example": "true"
					},
					"zip5": {
						"type": "string",
						"description": "The Zip 5 of the address",
						"example": "60616"
					}
				}
			},
			"marketInfo": {
				"type": "object",
				"properties": {
					"DSTMarketID": {
						"type": "object",
						"properties": {
							"franchiseTaxArea": {
								"type": "string"
							}
						}
					}
				}
			},
			"billingDetailsInfo": {
				"type": "object",
				"properties": {
					"csgDetails": {
						"type": "object",
						"properties": {
							"technicianArea": {
								"type": "string"
							}
						}
					}
				}
			},
			"locationIdentifierInfo": {
				"type": "object",
				"properties": {
					"eLocId": {
						"type": "string"
					},
					"naxId": {
						"type": "string"
					}
				}
			},
			"errors": {
				"id": "/properties/errors",
				"items": {
					"properties": {
						"code": {
							"type": "string",
							"example": "CONTRACT_VOILATION"
						},
						"message": {
							"type": "string",
							"example": "<Field Name> - Mandatory Field is missing in the request"
						}
					},
					"type": "object"
				},
				"type": "array"
			}
		}
	}