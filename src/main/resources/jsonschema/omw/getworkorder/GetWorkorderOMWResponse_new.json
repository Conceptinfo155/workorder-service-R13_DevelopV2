        "jobCustomer": {
          "type": "object",
          "description": "Details of the customer",
          "properties": {
            "customerId": {
              "type": "string",
              "description": "Customer ID",
              "example": "30370000001"
            },
            "siteId": {
              "type": "string",
              "description": "Site ID",
              "example": "456456"
            },
            "lastName": {
              "type": "string",
              "description": "First Name",
              "example": "Doe"
            },
            "firstName": {
              "type": "string",
