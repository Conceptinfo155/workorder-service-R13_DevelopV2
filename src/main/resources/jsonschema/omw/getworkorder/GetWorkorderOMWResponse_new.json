{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "workOrder": {
      "type": "object",
      "description": "Work Order details",
      "properties": {
        "solutionDetails": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "solutionType": {
                "type": "string",
                "example": "ETHERNET"
              },
              "orderType": {
                "type": "string",
                "example": "TC"
              },
              "transportType": {
                "type": "string",
                "example": "COAX"
              }
            }
          }
        },
        "job": {
          "type": "object",
          "description": "Description of the job",
          "properties": {
            "workOrderNumber": {
              "type": "string",
              "description": "Work order Number",
              "example": "ORION-098098"
            },
            "scheduleDate": {
              "type": "string",
              "description": "The scheduled date",
              "example": "2018-12-11"
            },
            "jobReasonCode": {
              "type": "string",
              "description": "The scheduled date",
              "example": "2018-12-11"
            },
            "technicianNum": {
              "type": "string",
              "description": "The scheduled date",
              "example": "2018-12-11"
            },
            "technicianId": {
              "type": "string",
              "description": "The assigned technician ID",
              "example": "2018-12-11"
            },
            "timeSlotStartDateTime": {
              "type": "string",
              "description": "Start date of the time slot",
              "example": "2018-12-11T12:00:00-08:00"
            },
            "timeSlotEndDateTime": {
              "type": "string",
              "description": "End date of the time slot",
              "example": "2018-12-11T14:00:00-08:00"
            },
            "timeSlotCd": {
              "type": "string",
              "description": "Time slot code",
              "example": "0"
            },
            "timeslotDescription": {
              "type": "string",
              "description": "Description of the time slot in local time",
              "example": "1200-1400"
            },
            "timeZoneOffset": {
              "type": "string",
              "description": "Time zone offset",
              "example": "480"
            },
            "callFirstPhoneNum": {
              "type": "string",
              "description": "the phone number to call first",
              "example": "55674567"
            },
            "troubleCallIndicator": {
              "type": "string",
              "description": "Indicator to say if its a trouble call",
              "example": "Y"
            },
            "jobComment": {
              "type": "string",
              "description": "Job comment added",
              "example": "TEST"
            },
            "orderComment": {
              "type": "string",
              "description": "Order Comment",
              "example": "test test"
            },
            "jobClassCd": {
              "type": "string",
              "description": "Job Class Code",
              "example": "S"
            },
            "jobTypeDesc": {
              "type": "string",
              "description": "Description of the Job type",
              "example": "UP BC SIP FIBER"
            },
            "SysPrinAgent": {
              "type": "string",
              "description": "The SPA of the location",
              "example": "821010000200"
            },
            "dispatcherStatusCd": {
              "type": "string",
              "description": "Dispatcher status code",
              "example": "R"
            },
            "jobUnits": {
              "type": "string",
              "description": "the effort required to complete the job",
              "example": "5"
            },
            "jobTypeCd": {
              "type": "string",
              "description": "JOb Type code",
              "example": "6E"
            },
            "jobNum": {
              "type": "string",
              "description": "Job Number",
              "example": "000068"
            },
            "fulfillmentCenter": {
              "type": "string",
              "description": "Fulfillment Center",
              "example": "182101001"
            },
            "JobCustomFields": {
              "$id": "#/items/properties/Job/properties/JobCustomFields",
              "type": "object",
